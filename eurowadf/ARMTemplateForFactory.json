{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eurowadf"
		},
		"ewdatalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ewdatalake'"
		},
		"sql_staging_adf_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_staging_adf'"
		},
		"sql_staging_adf_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-server-ew-demo.database.windows.net"
		},
		"sql_staging_adf_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ewdemo"
		},
		"sql_staging_adf_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "abene"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ewdatalake')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('ewdatalake_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/sql_staging_adf')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"server": "[parameters('sql_staging_adf_properties_typeProperties_server')]",
					"database": "[parameters('sql_staging_adf_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('sql_staging_adf_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('sql_staging_adf_password')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
		{
			"name": "[concat(parameters('factoryName'), '/stg_customers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzPgsql",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzurePostgreSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbt_abenes_stg",
					"table": "customers"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzPgsql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/stg_invoices')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzPgsql",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzurePostgreSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbt_abenes_stg",
					"table": "invoices"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzPgsql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/stg_payments')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzPgsql",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzurePostgreSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbt_abenes_stg",
					"table": "payments"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzPgsql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/datetime_convert_postingdate')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "adls_payments",
								"type": "DatasetReference"
							},
							"name": "adlspaymentsvalidated"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "adls_payments_formatted",
								"type": "DatasetReference"
							},
							"name": "adlsPaymentsFormatted"
						}
					],
					"transformations": [
						{
							"name": "postingdateFormatted",
							"description": "correct datetime postigdate to pgswl format"
						}
					],
					"scriptLines": [
						"source(output(",
						"          CompanyId as string,",
						"          CustomerId as string,",
						"          CountryId as string,",
						"          DocumentNumber as string,",
						"          DocumentType as string,",
						"          PostingDate as string,",
						"          Entry as string,",
						"          EntryType as string,",
						"          Amount as string,",
						"          InvoiceNumber as string,",
						"          InvoiceEntry as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> adlspaymentsvalidated",
						"adlspaymentsvalidated derive(PostingDate = toDate(PostingDate, 'dd.MM.yyyy')) ~> postingdateFormatted",
						"postingdateFormatted sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          CompanyId as string,",
						"          CustomerId as string,",
						"          CountryId as string,",
						"          DocumentNumber as string,",
						"          DocumentType as string,",
						"          PostingDate as string,",
						"          Entry as string,",
						"          EntryType as string,",
						"          Amount as string,",
						"          InvoiceNumber as string,",
						"          InvoiceEntry as string",
						"     ),",
						"     partitionFileNames:['DS3_Payments_Formatted.csv'],",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> adlsPaymentsFormatted"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/adls_payments')]",
				"[concat(variables('factoryId'), '/datasets/adls_payments_formatted')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Prepare stage')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Pipeline to trigger and monitor a DBT Cloud job via REST API.",
				"activities": [
					{
						"name": "DBT prepare stage",
						"type": "WebActivity",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30
						},
						"userProperties": [],
						"typeProperties": {
							"method": "POST",
							"headers": {
								"Authorization": {
									"value": "@{concat('Token ', pipeline().parameters.DbtApiToken)}",
									"type": "Expression"
								},
								"Content-Type": "application/json"
							},
							"url": {
								"value": "@{concat(pipeline().parameters.DbtBaseUrl, '/api/v2/accounts/', pipeline().parameters.AccountId, '/jobs/', pipeline().parameters.PrepSchemaJobId, '/run/')}",
								"type": "Expression"
							},
							"body": {
								"cause": "Triggered from ADF pipeline"
							}
						}
					},
					{
						"name": "Get Run ID",
						"type": "SetVariable",
						"dependsOn": [
							{
								"activity": "DBT prepare stage",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "dbtRunId",
							"value": {
								"value": "@string(activity('DBT prepare stage').output.data.id)",
								"type": "Expression"
							}
						}
					},
					{
						"name": "Until DBT Job Finished",
						"type": "Until",
						"dependsOn": [
							{
								"activity": "Get Run ID",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@or(equals(variables('dbtRunStatus'), 'Success'), equals(variables('dbtRunStatus'), 'Error'))",
								"type": "Expression"
							},
							"activities": [
								{
									"name": "Check DBT Run Status",
									"type": "WebActivity",
									"dependsOn": [],
									"policy": {
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"method": "GET",
										"headers": {
											"Authorization": {
												"value": "@{concat('Token ', pipeline().parameters.DbtApiToken)}",
												"type": "Expression"
											},
											"Content-Type": "application/json"
										},
										"url": {
											"value": "@{concat(pipeline().parameters.DbtBaseUrl, '/api/v2/accounts/', pipeline().parameters.AccountId, '/runs/', variables('dbtRunId'), '/')}",
											"type": "Expression"
										}
									}
								},
								{
									"name": "Get DBT Run Status",
									"type": "SetVariable",
									"dependsOn": [
										{
											"activity": "Check DBT Run Status",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"policy": {
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"variableName": "dbtRunStatus",
										"value": {
											"value": "@string(activity('Check DBT Run Status').output.data.status_humanized)",
											"type": "Expression"
										}
									}
								},
								{
									"name": "Wait 30s",
									"type": "Wait",
									"dependsOn": [
										{
											"activity": "Get DBT Run Status",
											"dependencyConditions": [
												"Succeeded"
											]
										}
									],
									"userProperties": [],
									"typeProperties": {
										"waitTimeInSeconds": 35
									}
								}
							],
							"timeout": "0.03:00:00"
						}
					},
					{
						"name": "If Success or Fail",
						"type": "IfCondition",
						"dependsOn": [
							{
								"activity": "Until DBT Job Finished",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@equals(variables('dbtRunStatus'), 'Success')",
								"type": "Expression"
							},
							"ifFalseActivities": [
								{
									"name": "Fail Pipeline",
									"type": "Fail",
									"dependsOn": [],
									"userProperties": [],
									"typeProperties": {
										"message": "DBT Cloud job failed. Check DBT Cloud run logs.",
										"errorCode": "400"
									}
								}
							],
							"ifTrueActivities": [
								{
									"name": "Move Raw data to customers",
									"description": "TRUNCATE TABLE dbt_abenes_stg.customers;\n",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "DelimitedTextSource",
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": true,
												"enablePartitionDiscovery": false
											},
											"formatSettings": {
												"type": "DelimitedTextReadSettings"
											}
										},
										"sink": {
											"type": "AzurePostgreSqlSink",
											"writeBatchSize": 1000000,
											"writeBatchTimeout": "00:30:00",
											"writeMethod": "CopyCommand"
										},
										"enableStaging": false,
										"translator": {
											"type": "TabularTranslator",
											"mappings": [
												{
													"source": {
														"name": "CustomerId",
														"type": "String",
														"physicalType": "String"
													},
													"sink": {
														"name": "customerid",
														"type": "String",
														"physicalType": "character varying"
													}
												},
												{
													"source": {
														"name": "CustomerName",
														"type": "String",
														"physicalType": "String"
													},
													"sink": {
														"name": "customername",
														"type": "String",
														"physicalType": "character varying"
													}
												},
												{
													"source": {
														"name": "CustomerCategory",
														"type": "String",
														"physicalType": "String"
													},
													"sink": {
														"name": "customercategory",
														"type": "String",
														"physicalType": "character varying"
													}
												}
											],
											"typeConversion": true,
											"typeConversionSettings": {
												"allowDataTruncation": true,
												"treatBooleanAsNumber": false
											}
										}
									},
									"inputs": [
										{
											"referenceName": "adls_customers",
											"type": "DatasetReference",
											"parameters": {}
										}
									],
									"outputs": [
										{
											"referenceName": "stg_customers",
											"type": "DatasetReference",
											"parameters": {}
										}
									]
								},
								{
									"name": "Move Raw data to invoices",
									"description": "TRUNCATE TABLE dbt_abenes_stg.invoices;\n",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "ExcelSource",
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": true,
												"enablePartitionDiscovery": false
											}
										},
										"sink": {
											"type": "AzurePostgreSqlSink",
											"writeBatchSize": 1000000,
											"writeBatchTimeout": "00:30:00",
											"writeMethod": "CopyCommand"
										},
										"enableStaging": false,
										"translator": {
											"type": "TabularTranslator",
											"mappings": [
												{
													"source": {
														"name": "CompanyId",
														"type": "String",
														"physicalType": "String"
													},
													"sink": {
														"name": "companyid",

{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eurowadf"
		},
		"AzPgsql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzPgsql'"
		},
		"adls_raw_adf_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_raw_adf'"
		},
		"ewdatalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ewdatalake'"
		},
		"AzPgsql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pgdqleurow.postgres.database.azure.com"
		},
		"AzPgsql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ewdemo"
		},
		"AzPgsql_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "abene"
		},
		"adls_raw_adf_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eurowdemo.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzPgsql')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzurePostgreSql",
				"version": "2.0",
				"typeProperties": {
					"server": "[parameters('AzPgsql_properties_typeProperties_server')]",
					"port": "5432",
					"database": "[parameters('AzPgsql_properties_typeProperties_database')]",
					"sslMode": 2,
					"username": "[parameters('AzPgsql_properties_typeProperties_username')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('AzPgsql_password')]"
					},
					"authenticationType": "Basic"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/adls_raw_adf')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('adls_raw_adf_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('adls_raw_adf_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ewdatalake')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('ewdatalake_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/adls_customers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "adls_raw_adf",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "DS1_Customers.txt",
						"folderPath": "raw",
						"fileSystem": "datalake"
					},
					"columnDelimiter": "\t",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "CustomerId\tCustomerName\tCustomerCategory",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/adls_raw_adf')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/adls_invoices')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "adls_raw_adf",
					"type": "LinkedServiceReference"
				},
				"annotations": [],

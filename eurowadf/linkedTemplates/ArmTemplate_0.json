		{
			"name": "[concat(parameters('factoryName'), '/adls_payments_formatted')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "adls_raw_adf",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "DS3_Payments_Formatted.csv",
						"folderPath": "raw",
						"fileSystem": "datalake"
					},
					"columnDelimiter": ";",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "CompanyId",
						"type": "String"
					},
					{
						"name": "CustomerId",
						"type": "String"
					},
					{
						"name": "CountryId",
						"type": "String"
					},
					{
						"name": "DocumentNumber",
						"type": "String"
					},
					{
						"name": "DocumentType",
						"type": "String"
					},
					{
						"name": "PostingDate",
						"type": "String"
					},
					{
						"name": "Entry",

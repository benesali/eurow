{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "eurowadf"
		},
		"ewdatalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ewdatalake'"
		},
		"sql_staging_adf_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sql_staging_adf'"
		},
		"sql_staging_adf_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-server-ew-demo.database.windows.net"
		},
		"sql_staging_adf_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ewdemo"
		},
		"sql_staging_adf_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "abene"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "eurowadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ewdatalake_connectionString": {
						"value": "[parameters('ewdatalake_connectionString')]"
					},
					"sql_staging_adf_password": {
						"value": "[parameters('sql_staging_adf_password')]"
					},
					"sql_staging_adf_properties_typeProperties_server": {
						"value": "[parameters('sql_staging_adf_properties_typeProperties_server')]"
					},
					"sql_staging_adf_properties_typeProperties_database": {
						"value": "[parameters('sql_staging_adf_properties_typeProperties_database')]"
					},
					"sql_staging_adf_properties_typeProperties_userName": {
						"value": "[parameters('sql_staging_adf_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}